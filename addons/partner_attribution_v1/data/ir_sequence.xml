<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <!-- Partner Code -->
    <record id="seq_partner_attribution_partner_code" model="ir.sequence">
        <field name="name">Partner Code</field>
        <field name="code">partner_attribution.partner_code</field>
        <field name="prefix">PC-</field>
        <field name="padding">6</field>
        <field name="company_id" eval="False"/>
        <field name="implementation">standard</field>
        <field name="active">True</field>
    </record>

    <!-- Partner UID -->
    <record id="seq_partner_attribution_partner_uid" model="ir.sequence">
        <field name="name">Partner UID</field>
        <field name="code">partner_attribution.partner_uid</field>
        <field name="prefix">PID-</field>
        <field name="padding">6</field>
        <field name="company_id" eval="False"/>
        <field name="implementation">standard</field>
        <field name="active">True</field>
    </record>

</odoo>